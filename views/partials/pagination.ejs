<!-- pagination css -->
<style>
  .pagination {
    padding: 8px 16px;
    border: 1px solid #ccc;
    color: #333;
    font-weight: bold;
  }
</style>

<div align="center">
  <br />
  <% total_pages = Math.ceil(data.count/12) %> <% for(var i=1; i < total_pages;
  i++ ){ %>
  <a class="pagination" href="bookshelf?page=<%= i %>"><%= i %></a>
  <% } %>
</div>
